<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SampleActivityRecognition.Views.Menu.MasterView"
             Title="MasterView">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="ButtonRunningApp"
                    Text="RunningApp"
                    Command="{Binding RunningAppCommand}"/>
            <Button x:Name="ButtonTrackerApp"
                    Text="TrackerApp"
                    Command="{Binding TrackerAppCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>